    <changeSet id="20211811_2112" author="arturo.nieves">
        <sql>
            CREATE TABLE User(
                 id  INT(11) NOT NULL AUTO_INCREMENT,
                 name varchar(30),
                 lastname varchar(30),
                 username varchar(30) UNIQUE,
                 password varchar (60),
                 PRIMARY KEY (id)
            )
        </sql>
    </changeSet>
